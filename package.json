{
  "name": "kce",
  "version": "0.1.0",
  "author": "Arc",
  "description": "KCEmulator",
  "private": true,
  "scripts": {
    "bgmmake": "node -e \"require('./src/lib/kcdbmaker').runBgmMakeFromNode()\"",
    "semake": "node -e \"require('./src/lib/kcdbmaker').runSEMakeFromNode()\"",
    "kcmake": "node -e \"require('./src/lib/kcdbmaker').runKCMakeFromNode()\"",
    "kcmakeall": "node -e \"require('./src/lib/kcdbmaker').runAllKCMakeFromNode()\"",
    "clean": "node -e \"require('./src/lib/utils').deleteAllRecursive('./dist_electron')\"",
    "vue:serve": "vue-cli-service serve",
    "vue:build": "vue-cli-service build",
    "vue:lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build --win",
    "electron:serve": "vue-cli-service electron:serve --win",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "dev": "vue-cli-service electron:serve",
    "pack": "asar pack ship/yamato packed/ship/yamato.asar",
    "build": "npm-run-all clean electron:build"
  },
  "main": "background.js",
  "dependencies": {
    "core-js": "^3.6.4",
    "vue": "^2.6.11",
    "vuex": "^3.3.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "asar": "^3.0.3",
    "babel-eslint": "^10.1.0",
    "electron": "^5.0.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-vue": "^6.2.2",
    "less-loader": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "vue-cli-plugin-electron-builder": "~1.4.6",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "globals": {
      "log": true,
      "logError": true,
      "__dev": true,
      "__resources": true,
      "__ship": true,
      "__roomBackground": true,
      "__mobileMode": true,
      "__static": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}